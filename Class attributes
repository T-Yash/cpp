#include <iostream>
#include <string>
using namespace std;
class Student {
public:
    int rollNo;
    string stdName;
    float perc;
};

int main()
{

    Student std;
    
    std.rollNo = 101;
    std.stdName = "abc";
    std.perc =90;
    cout << "Student's Roll No.: " << std.rollNo << "\n";
    cout << "Student's Name: " << std.stdName << "\n";
    cout << "Student's Percentage: " << std.perc << "\n";

    return 0;
}
