#include <iostream> 
#include <string>
using namespace std;
class employ {
public:
    int idno;
    string stdName;
    float salary;
};

int main()
{

     employ std;
    
    std.idno = 555;
    std.stdName = "abc";
    std.salary =90;
    cout << "company id number.: " << std.idno << "\n";
    cout << "Name: " << std.stdName << "\n";
    cout << "salary in (thosand): " << std.salary << "\n";

    return 0;
}
